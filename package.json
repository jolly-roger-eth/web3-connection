{
	"name": "web3-connection",
	"version": "0.1.30",
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"peerDependencies": {
		"svelte": "^4.0.0"
	},
	"devDependencies": {
		"@changesets/cli": "^2.27.1",
		"@sveltejs/adapter-static": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/package": "^2.2.6",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@tailwindcss/forms": "^0.5.7",
		"@tailwindcss/typography": "^0.5.10",
		"@types/node": "^20.11.16",
		"abitype": "^1.0.0",
		"as-soon": "^0.0.8",
		"autoprefixer": "^10.4.17",
		"daisyui": "^4.6.1",
		"eip-1193": "^0.4.7",
		"jsdom": "^24.0.0",
		"named-logs-console": "^0.3.0",
		"postcss": "^8.4.34",
		"postcss-load-config": "^5.0.2",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.1.2",
		"publint": "^0.2.7",
		"set-defaults": "^0.0.2",
		"svelte": "^4.2.10",
		"svelte-check": "^3.6.3",
		"svelte-preprocess": "^5.1.3",
		"tailwindcss": "^3.4.1",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"vite": "^5.0.0",
		"vitest": "^1.0.0",
		"web3w-walletconnect-loader": "^0.4.3"
	},
	"scripts": {
		"prepare": "set-defaults .vscode",
		"dev": "vite dev",
		"dev:build": "as-soon -w src/lib pnpm run dev:package",
		"dev:package": "svelte-package -o dist-tmp && cp -R dist-tmp/* dist/",
		"build": "pnpm run package",
		"preview": "vite preview",
		"package:dev": "svelte-kit sync && svelte-package --watch",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check .",
		"format": "prettier --plugin-search-dir . --write .",
		"svelte-check": "svelte-check",
		"test": "vitest --silent=false",
		"coverage": "vitest run --coverage"
	},
	"dependencies": {
		"named-logs": "^0.2.2"
	}
}